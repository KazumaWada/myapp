    <div id="popular" class="tab-pane">
    <%# ここから2 %>
    <% if @posts_get_likes.any? %>
    <% @posts_get_likes.each do |post_get_like| %>

    <%= link_to post_path(post_get_like), class: 'text-body under' do %> 
    <div class="card mt-4">
    <%= image_tag post_get_like.image.url, class: "img-fluid" %>
     <% end %><%# posts#show %>
     <div class="card-body">
     <h5 class="card-title">
     <%# name, avatar  %>
     <span class="small">
     <%= link_to user_path(post_get_like.user), class: 'under' do %>
     <% if post_get_like.user.avatar? %>
     <%= image_tag post_get_like.user.avatar.url, class: "rounded-circle mb-3", width: "40px", height: "40px" %>
     <% else %>
     <%= image_tag "the-main.jpg", class: "rounded-circle", width: "40px", height: "40px" %>
     <% end %>
    <% end %>
    <%= post_get_like.user.name %>
    </span><br>
    <%# covid  %>
    記事内容: <span class="small bg-warning">
    <%= post_get_like.covid %>
    </span>
    <%# title  %>
    <h3><%= post_get_like.title %></h3> 
    <%# tag, description  %>
    <span><%= render_with_hashtags(post_get_like.content.truncate(85)) %><%= link_to '続きを読む', post_path(post_get_like) %></span> 
   </h5>   
    <p class="card-text">
        <%# likes %>
        <h3>
         <div class="post-like">
        <%= render 'shared/likes', post: post_get_like %> 
        </div>
        </h3>
    <%# impression %>
       <span>
       <i class="fas fa-eye"><%= post_get_like.impressions_count %></i>
      </span>
     <span>
      <%= time_ago_in_words(post_get_like.created_at) %> 前
      </span>
        </p>
      </div><%# card-body %>
    </div><%# card %>
     
    <% end %><%# each %>
  
     <% end %> <%# ifpostsany? %>
  <div class="mt-4">
    <%= paginate @posts_get_likes %>  
  </div> 

    <%# ここまで2 %>
    </div><%# popular %>